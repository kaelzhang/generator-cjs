{
  "name": "generator-cjs",
  "version": "1.3.1",
  "description": "Yeoman generator to create a CommonJS project",
  "scripts": {
    "test": "nyc ava --timeout=10s",
    "test-no-report": "NODE_DEBUG=generator-cjs nyc ava --timeout=10s --verbose",
    "lint": "eslint .",
    "posttest": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "files": [
    "generators"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kaelzhang/generator-cjs.git"
  },
  "keywords": [
    "generator-cjs",
    "generator",
    "yeoman generator",
    "commonjs",
    "generator commonjs"
  ],
  "engines": {
    "node": ">= 6"
  },
  "ava": {
    "files": [
      "test/*.test.js"
    ]
  },
  "author": "kaelzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaelzhang/generator-cjs/issues"
  },
  "devDependencies": {
    "ava": "^4.0.0",
    "codecov": "^3.3.0",
    "nyc": "^13.3.0",
    "eslint-config-ostai": "^3.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint": "^5.16.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "common-generator-prompts": "^2.0.0",
    "mustache": "^3.0.1",
    "scaffold-generator": "^3.0.7",
    "yeoman-generator": "^3.2.0",
    "yosay": "^2.0.2"
  }
}
